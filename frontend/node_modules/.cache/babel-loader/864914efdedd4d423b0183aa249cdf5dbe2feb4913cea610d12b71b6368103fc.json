ete,\n      formData,\n      fetchData,\n      showAddDialog,\n      showEditDialog,\n      showDeleteDialog,\n      confirmDelete,\n      ref,\n      onMounted,\n      get request() {\n        return request;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","request","name","currentPage","pageSize","total","data","arr","dialogFormVisible","deleteDialogVisible","isEditing","rowToDelete","formData","id","phone","email","socialid","collect","fetchData","response","get","params","pageNum","value","trim","list","showAddDialog","showEditDialog","row","showDeleteDialog","confirmDelete","delete"],"sources":["D:/大学资料/软件工程/项目/test/demo/vue/src/views/HomeView.vue"],"sourcesContent":["<template>\r\n  <!-- 顶部区域 -->\r\n  <div style=\"margin: 10px 10px; display: flex;\">\r\n    <el-input v-model=\"name\" style=\"width: 20%;\" placeholder=\"输入姓名\"></el-input>\r\n    <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"handleSearch\">搜索</el-button>\r\n    <el-button type=\"success\" style=\"margin-left: 5px\" @click=\"handleCollect\">收藏的联系人</el-button>\r\n    <el-button plain style=\"margin-left: auto;\" @click=\"showAddDialog\">+</el-button>\r\n    <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"handleExport\">导出数据</el-button>\r\n    <el-upload\r\n        :action=\"$baseUrl + '/user/import'\"\r\n        accept=\".xlsx\"\r\n        :auto-upload=\"true\"\r\n        :show-file-list=\"false\"\r\n        style=\"margin-left: 5px;\"\r\n        :headers=\"headers\"\r\n        :data=\"uploadParams\"\r\n        @success=\"handleImportSuccess\"\r\n        @error=\"handleImportError\"\r\n    >\r\n      <el-button type=\"primary\">导入数据</el-button>\r\n    </el-upload>\r\n  </div>\r\n\r\n  <!-- 表格区域 -->\r\n  <div style=\"padding: 10px;\">\r\n    <el-table :data=\"data.arr\" border :row-class-name=\"rowClassName\">\r\n      <el-table-column type=\"selection\" />\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"55\" />\r\n      <el-table-column prop=\"name\" label=\"姓名\" />\r\n      <el-table-column prop=\"phone\" label=\"手机号码\" />\r\n      <el-table-column prop=\"email\" label=\"邮箱\" />\r\n      <el-table-column prop=\"socialid\" label=\"社媒账号\" />\r\n\r\n      <el-table-column label=\"\" width=\"150\">\r\n        <template #default=\"scope\">\r\n          <el-button @click=\"showEditDialog(scope.row)\" size=\"small\" type=\"primary\">编辑</el-button>\r\n          <el-button @click=\"showDeleteDialog(scope.row)\" size=\"small\" type=\"danger\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </div>\r\n\r\n  <div class=\"example-pagination-block\">\r\n    <el-pagination\r\n        v-model:current-page=\"currentPage\"\r\n        :page-size=\"pageSize\"\r\n        :total=\"total\"\r\n        layout=\"prev, pager, next, sizes\"\r\n        :page-sizes=\"[10, 100]\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n    />\r\n  </div>\r\n\r\n  <!-- 新增/编辑用户弹窗 -->\r\n  <el-dialog :title=\"isEditing ? '编辑' : '新增'\" v-model=\"dialogFormVisible\">\r\n    <el-form :model=\"formData\" label-width=\"80px\">\r\n      <el-form-item label=\"姓名\">\r\n        <el-input v-model=\"formData.name\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"手机号码\">\r\n        <el-input v-model=\"formData.phone\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"邮箱\">\r\n        <el-input v-model=\"formData.email\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"社媒账号\">\r\n        <el-input v-model=\"formData.socialid\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"是否收藏\">\r\n        <el-radio-group v-model=\"formData.collect\">\r\n          <el-radio :label=\"true\">是</el-radio>\r\n          <el-radio :label=\"false\">否</el-radio>\r\n        </el-radio-group>\r\n      </el-form-item>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"dialogFormVisible = false\">取消</el-button>\r\n      <el-button type=\"primary\" @click=\"isEditing ? handleUpdate() : handleAdd()\">确定</el-button>\r\n    </div>\r\n  </el-dialog>\r\n\r\n  <!-- 删除确认弹窗 -->\r\n  <el-dialog title=\"确认删除\" v-model=\"deleteDialogVisible\">\r\n    <span>确定要删除该用户吗？</span>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"deleteDialogVisible = false\">取消</el-button>\r\n      <el-button type=\"primary\" @click=\"confirmDelete\">确认</el-button>\r\n    </div>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue';\r\nimport request from '@/utils/request';\r\n\r\nconst name = ref('');\r\nconst currentPage = ref(1);\r\nconst pageSize = ref(10);\r\nconst total = ref(0);\r\nconst data = ref({ arr: [] });\r\nconst dialogFormVisible = ref(false);\r\nconst deleteDialogVisible = ref(false);\r\nconst isEditing = ref(false);\r\nconst rowToDelete = ref(null); // 存储待删除行的数据\r\n\r\nconst formData = ref({\r\n  id: null,\r\n  name: '',\r\n  phone: '',\r\n  email: '',\r\n  socialid: '',\r\n  collect: ''\r\n});\r\n\r\nconst fetchData = async () => {\r\n  const response = await request.get('/user/selectByPage', {\r\n    params: {\r\n      pageNum: currentPage.value,\r\n      pageSize: pageSize.value,\r\n      name: name.value.trim()\r\n    }\r\n  });\r\n  data.value.arr = response.data.list;\r\n  total.value = response.data.total;\r\n};\r\n\r\nconst showAddDialog = () => {\r\n  isEditing.value = false;\r\n  formData.value = { id: null, name: '', phone: '', email: '', socialid: '', collect: '' };\r\n  dialogFormVisible.value = true;\r\n};\r\n\r\nconst showEditDialog = (row) => {\r\n  isEditing.value = true;\r\n  formData.value = { ...row };\r\n  dialogFormVisible.value = true;\r\n};\r\n\r\nconst showDeleteDialog = (row) => {\r\n  rowToDelet