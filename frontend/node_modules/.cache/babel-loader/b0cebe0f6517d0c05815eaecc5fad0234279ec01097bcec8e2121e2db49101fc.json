{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport request from \"@/utils/request\";\nexport default {\n  __name: 'HomeView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const name = ref('');\n    const currentPage = ref(1); // 当前页码\n    const pageSize = ref(10); // 每页显示的条数\n    const total = ref(50); // 总条数\n    const data = ref({\n      arr: []\n    });\n    const visible = ref(false);\n    const dialogVisible = ref(false); // Controls form dialog visibility\n\n    // Form data\n    const form = ref({\n      username: '',\n      password: '',\n      name: '',\n      phone: '',\n      email: '',\n      address: '',\n      avatar: ''\n    });\n    const handleSizeChange = newSize => {\n      pageSize.value = newSize;\n    };\n    const handleCurrentChange = newPage => {\n      currentPage.value = newPage; // 更新当前页码\n    };\n\n    // Fetch data function\n    const fetchData = async () => {\n      try {\n        const response = await request.get('/user/selectAll');\n        data.value.arr = response.data;\n      } catch (error) {\n        console.error('Error fetching data:', error);\n      }\n    };\n\n    // Function to handle the search\n    const handleSearch = async () => {\n      if (name.value.trim()) {\n        try {\n          const response = await request.get(`/user/selectByName/${name.value.trim()}`);\n          data.value.arr = response.data;\n        } catch (error) {\n          console.error('Error fetching data by name:', error);\n        }\n      } else {\n        // If name input is empty, fetch all data\n        fetchData();\n      }\n    };\n\n    // Function to handle the form submission\n    const submitForm = async () => {\n      try {\n        await request.post('/user/add', form.value);\n        dialogVisible.value = false; // Close the dialog after successful submission\n        fetchData(); // Refresh data to include the new contact\n        form.value = {\n          username: '',\n          passwo