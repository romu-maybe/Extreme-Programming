ata.value.arr = response.data;\r\n  } catch (error) {\r\n    console.error('Error fetching data:', error);\r\n  }\r\n};\r\n\r\n// Function to handle the search\r\nconst handleSearch = async () => {\r\n  if (name.value.trim()) {\r\n    try {\r\n      const response = await request.get(`/user/selectByName/${name.value.trim()}`);\r\n      data.value.arr = response.data;\r\n    } catch (error) {\r\n      console.error('Error fetching data by name:', error);\r\n    }\r\n  } else {\r\n    // If name input is empty, fetch all data\r\n    fetchData();\r\n  }\r\n};\r\n\r\n// Function to handle the form submission\r\nconst submitForm = async () => {\r\n  try {\r\n    await request.post('/user/add', form.value);\r\n    dialogVisible.value = false; // Close the dialog after successful submission\r\n    fetchData(); // Refresh data to include the new contact\r\n    form.value = { username: '', password: '', name: '', phone: '', email: '', address: '', avatar: '' }; // Reset form\r\n  } catch (error) {\r\n    console.error('Error adding new contact:', error);\r\n  }\r\n};\r\n\r\nconst handleEdit = (row) => {\r\n  console.log('Editing row:', row);\r\n};\r\n\r\nconst handleDelete = (row) => {\r\n  console.log('Deleting row:', row);\r\n  visible.value = true;\r\n};\r\n\r\nonMounted(fetchData);\r\n</script>\r\n"],"mappings":"AAmFA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,OAAO,MAAM,iBAAiB;;;;;;;IAErC,MAAMC,IAAI,GAAGH,GAAG,CAAC,EAAE,CAAC;IACpB,MAAMI,WAAW,GAAGJ,GAAG,CAAC,CAAC,CAAC,CAAC,CAAC;IAC5B,MAAMK,QAAQ,GAAGL,GAAG,CAAC,EAAE,CAAC,CAAC,CAAC;IAC1B,MAAMM,KAAK,GAAGN,GAAG,CAAC,EAAE,CAAC,CAAC,CAAC;IACvB,MAAMO,IAAI,GAAGP,GAAG,CAAC;MAAEQ,GAAG,EAAE;IAAG,CAAC,CAAC;IAC7B,MAAMC,OAAO,GAAGT,GAAG,CAAC,KAAK,CAAC;IAC1B,MAAMU,aAAa,GAAGV,GAAG,CAAC,KAAK,CAAC,CAAC,CAAC;;IAElC;IACA,MAAMW,IAAI,GAAGX,GAAG,CAAC;MACfY,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZV,IAAI,EAAE,EAAE;MACRW,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE;IACV,CAAC,CAAC;IAEF,MAAMC,gBAAgB,GAAIC,OAAO,IAAK;MACpCd