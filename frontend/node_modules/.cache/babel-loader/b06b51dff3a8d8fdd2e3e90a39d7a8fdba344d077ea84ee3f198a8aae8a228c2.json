rue;\n    };\n\n    // 新增用户\n    const handleAdd = async () => {\n      try {\n        await request.post('/user/add', formData.value);\n        dialogFormVisible.value = false;\n        fetchData();\n      } catch (error) {\n        console.error('Error adding user:', error);\n      }\n    };\n\n    // 更新用户\n    const handleUpdate = async () => {\n      try {\n        await request.put('/user/update', formData.value);\n        dialogFormVisible.value = false;\n        fetchData();\n      } catch (error) {\n        console.error('Error updating user:', error);\n      }\n    };\n\n    // 删除用户\n    const confirmDelete = async id => {\n      try {\n        await request.delete(`/user/delete/${id}`);\n        fetchData();\n      } catch (error) {\n        console.error('Error deleting user:', error);\n      }\n    };\n    onMounted(fetchData);\n    const __returned__ = {\n      name,\n      currentPage,\n      pageSize,\n      total,\n      data,\n      visible,\n      dialogFormVisible,\n      isEditing,\n      formData,\n      handleSizeChange,\n      handleCurrentChange,\n      fetchData,\n      handleSearch,\n      showAddDialog,\n      showEditDialog,\n      handleAdd,\n      handleUpdate,\n      confirmDelete,\n      ref,\n      onMounted,\n      get request() {\n        return request;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","request","name","currentPage","pageSize","total","data","arr","visible","dialogFormVisible","isEditing","formData","id","username","password","phone","email","address","handleSizeChange","newSize","value","handleCurrentChange","newPage","fetchData","response","get","error","console","handleSearch","trim","showAddDialog","showEditDialog","row","handleAdd","post","handleUpdate","put","confirmDelete","delete"],"sources":["D:/大学资料/软件工程/项目/test/demo/vue/src/views/HomeView.vu