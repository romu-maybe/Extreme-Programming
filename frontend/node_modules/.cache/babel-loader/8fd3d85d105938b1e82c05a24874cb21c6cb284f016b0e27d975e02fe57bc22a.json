{"ast":null,"code":"import { ref } from 'vue';\nimport request from \"@/utils/request\";\nexport default {\n  name: 'MyHeader',\n  setup() {\n    const dialogVisible = ref(false);\n    const form = ref({\n      username: '',\n      password: '',\n      name: '',\n      phone: '',\n      email: '',\n      address: '',\n      avatar: ''\n    });\n    const submitForm = async () => {\n      try {\n        await request.post('/user/add', form.value);\n        dialogVisible.value = false; // Close the dialog after successful submission\n        // Optionally, reset the form fields\n        form.value = {\n          username: '',\n          password: '',\n          name: '',\n          phone: '',\n          email: '',\n          address: '',\n          avatar: ''\n        };\n        // Optionally, trigger data refresh to show the newly added contact\n        console.log(\"New contact added successfully.\");\n      } catch (error) {\n        console.error(\"Error adding new contact:\", error);\n      }\n    };\n    return {\n      dialogVisible,\n      form,\n      submitForm\n    };\n  }\n};","map":{"version":3,"names":["ref","request","name","setup","dialogVisible","form","username","password","phone","email","address","avatar","submitForm","post","value","console","log","error"],"sources":["D:\\大学资料\\软件工程\\项目\\test\\demo\\vue\\src\\components\\header.vue"],"sourcesContent":["<template>\r\n  <div style=\"height: 50px; line-height: 50px; border-bottom: 1px solid #ccc; display: flex\">\r\n    <div style=\"width: 200px; padding-left: 30px; font-weight: bold; color: dodgerblue;\">联系人</div>\r\n    <div style=\"flex: 1\"></div>\r\n    <div style=\"width: 100px;\">\r\n      <el-button type=\"primary\" @click=\"dialogVisible = true\">+</el-button>\r\n    </div>\r\n  </div>\r\n\r\n  <!-- Form Dialog -->\r\n  <el-dialog title=\"Add New Contact\" :visible.sync=\"dialogVisible\" width=\"30%\">\r\n    <el-form :model=\"form\" label-width=\"80px\">\r\n      <el-form-item label=\"Username\">\r\n        <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"Password\">\r\n        <el-input v-model=\"form.password\" type=\"password\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"Name\">\r\n        <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"Phone\">\r\n        <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"Email\">\r\n        <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"Address\">\r\n        <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"Avatar\">\r\n        <el-input v-model=\"form.avatar\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n    </el-form>\r\n    <template #footer>\r\n      <el-button @click=\"dialogVisible = false\">Cancel</el-button>\r\n      <el-button type=\"primary\" @click=\"submitForm\">Submit</el-button>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue';\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: 'MyHeader',\r\n  setup() {\r\n    const dialogVisible = ref(false);\r\n    const form = ref({\r\n      username: '',\r\n      password: '',\r\n      name: '',\r\n      phone: '',\r\n      email: '',\r\n      address: '',\r\n      avatar: ''\r\n    });\r\n\r\n    const submitForm = async () => {\r\n      try {\r\n        await request.post('/user/add', form.value);\r\n        dialogVisible.value = false; // Close the dialog after successful submission\r\n        // Optionally, reset the form fields\r\n        form.value = { username: '', password: '', name: '', phone: '', email: '', address: '', avatar: '' };\r\n        // Optionally, trigger data refresh to show the newly added contact\r\n        console.log(\"New contact added successfully.\");\r\n      } catch (error) {\r\n        console.error(\"Error adding new contact:\", error);\r\n      }\r\n    };\r\n\r\n    return {\r\n      dialogVisible,\r\n      form,\r\n      submitForm\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Here you can add your styles */\r\n</style>\r\n"],"mappings":"AA0CA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,OAAM,MAAO,iBAAiB;AAErC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAIJ,GAAG,CAAC,KAAK,CAAC;IAChC,MAAMK,IAAG,GAAIL,GAAG,CAAC;MACfM,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZL,IAAI,EAAE,EAAE;MACRM,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,EAAE;MACXC,MAAM,EAAE;IACV,CAAC,CAAC;IAEF,MAAMC,UAAS,GAAI,MAAAA,CAAA,KAAY;MAC7B,IAAI;QACF,MAAMX,OAAO,CAACY,IAAI,CAAC,WAAW,EAAER,IAAI,CAACS,KAAK,CAAC;QAC3CV,aAAa,CAACU,KAAI,GAAI,KAAK,EAAE;QAC7B;QACAT,IAAI,CAACS,KAAI,GAAI;UAAER,QAAQ,EAAE,EAAE;UAAEC,QAAQ,EAAE,EAAE;UAAEL,IAAI,EAAE,EAAE;UAAEM,KAAK,EAAE,EAAE;UAAEC,KAAK,EAAE,EAAE;UAAEC,OAAO,EAAE,EAAE;UAAEC,MAAM,EAAE;QAAG,CAAC;QACpG;QACAI,OAAO,CAACC,GAAG,CAAC,iCAAiC,CAAC;MAChD,EAAE,OAAOC,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;MACnD;IACF,CAAC;IAED,OAAO;MACLb,aAAa;MACbC,IAAI;MACJO;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}