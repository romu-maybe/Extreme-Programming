{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport request from \"@/utils/request\";\nexport default {\n  __name: 'HomeView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const name = ref('');\n    const currentPage = ref(1);\n    const pageSize = ref(10);\n    const total = ref(0);\n    const data = ref({\n      arr: []\n    });\n    const dialogFormVisible = ref(false);\n    const deleteDialogVisible = ref(false); // 控制删除确认弹窗\n    const isEditing = ref(false);\n    const deleteId = ref(null); // 保存待删除的用户ID\n\n    // 表单数据\n    const formData = ref({\n      id: null,\n      username: '',\n      password: '',\n      name: '',\n      phone: '',\n      email: ''\n    });\n    const handleSizeChange = newSize => {\n      pageSize.value = newSize;\n      fetchData(); // 更新数据\n    };\n    const handleCurrentChange = newPage => {\n      currentPage.value = newPage;\n      fetchData(); // 更新数据\n    };\n\n    // 获取分页数据\n    const fetchData = async () => {\n      try {\n        const response = await request.get('/user/selectByPage', {\n          params: {\n            pageNum: currentPage.value,\n            pageSize: pageSize.value,\n            username: '',\n            name: name.value.trim()\n          }\n        });\n        data.value.arr = response.data.list;\n        total.value = response.data.total;\n      } catch (error) {\n        console.error('Error fetching data:', error);\n      }\n    };\n\n    // 搜索功能\n    const handleSearch = () => {\n      currentPage.value = 1;\n      fetchData();\n    };\n\n    // 显示新增弹窗\n    const showAddDialog = () => {\n      isEditing.value = false;\n      formData.value = {\n        id: null,\n        username: '',\n        password: '',\n        name: '',\n        phone: '',\n        email: ''\n      };\n      dialogFormVisible.value = true;\n    };\n\n    // 显示编辑弹窗\n    const showEditDialog = row => {\n      isEditing.value = true;\n      formData.value = {\n        ...row\n      };\n      dialogFormVisible.value = true;\n    };\n\n    // 显示删除确认弹窗\n    const showDeleteDialog = id => {\n      deleteId.value = id;\n      deleteDialogVisible.value = true;\n    };\n\n    // 确认删除用户\n    const confirmDelete = async () => {\n      try {\n        await request.delete(`/user/delete/${deleteId.value}`);\n        deleteDialogVisible.value = false;\n        deleteId.value = null; // 清除删除ID\n        fetchData();\n      } catch (error) {\n        console.error('Error deleting user:', error);\n      }\n    };\n    onMounted(fetchData);\n    const __returned__ = {\n      name,\n      currentPage,\n      pageSize,\n      total,\n      data,\n      dialogFormVisible,\n      deleteDialogVisible,\n      isEditing,\n      deleteId,\n      formData,\n      handleSizeChange,\n      handleCurrentChange,\n      fetchData,\n      handleSearch,\n      showAddDialog,\n      showEditDialog,\n      showDeleteDialog,\n      confirmDelete,\n      ref,\n      onMounted,\n      get request() {\n        return request;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","request","name","currentPage","pageSize","total","data","arr","dialogFormVisible","deleteDialogVisible","isEditing","deleteId","formData","id","username","password","phone","email","handleSizeChange","newSize","value","fetchData","handleCurrentChange","newPage","response","get","params","pageNum","trim","list","error","console","handleSearch","showAddDialog","showEditDialog","row","showDeleteDialog","confirmDelete","delete"],"sources":["D:/大学资料/软件工程/项目/test/demo/vue/src/views/HomeView.vue"],"sourcesContent":["<template>\r\n  <!-- 搜索区域 -->\r\n  <div style=\"margin: 10px 10px; display: flex;\">\r\n    <el-input v-model=\"name\" style=\"width: 20%;\" placeholder=\"输入姓名\"></el-input>\r\n    <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"handleSearch\">搜索</el-button>\r\n    <el-button type=\"primary\" plain style=\"margin-left: auto;\" @click=\"showAddDialog\">+</el-button>\r\n  </div>\r\n\r\n  <!-- 主体区域 -->\r\n  <div style=\"padding: 10px;\">\r\n    <el-table :data=\"data.arr\" border stripe>\r\n      <el-table-column type=\"selection\" />\r\n      <el-table-column prop=\"id\" label=\"编号\" width=\"55\" />\r\n      <el-table-column prop=\"name\" label=\"姓名\" />\r\n      <el-table-column prop=\"phone\" label=\"手机号码\" />\r\n      <el-table-column prop=\"email\" label=\"邮箱\" />\r\n      <el-table-column label=\"\" width=\"150\">\r\n        <template #default=\"scope\">\r\n          <el-button @click=\"showEditDialog(scope.row)\" size=\"small\" type=\"primary\">编辑</el-button>\r\n          <el-button @click=\"showDeleteDialog(scope.row.id)\" size=\"small\" type=\"danger\">删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </div>\r\n\r\n  <div class=\"example-pagination-block\">\r\n    <el-pagination\r\n        v-model:current-page=\"currentPage\"\r\n        :page-size=\"pageSize\"\r\n        :total=\"total\"\r\n        layout=\"prev, pager, next, sizes\"\r\n        :page-sizes=\"[10, 20, 50, 100]\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n    />\r\n  </div>\r\n\r\n  <!-- 新增/编辑用户弹窗 -->\r\n  <el-dialog :title=\"isEditing ? '编辑用户' : '新增用户'\" v-model=\"dialogFormVisible\">\r\n    <el-form :model=\"formData\" label-width=\"80px\">\r\n      <el-form-item label=\"姓名\">\r\n        <el-input v-model=\"formData.name\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"手机号码\">\r\n        <el-input v-model=\"formData.phone\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"邮箱\">\r\n        <el-input v-model=\"formData.email\"></el-input>\r\n      </el-form-item>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"dialogFormVisible = false\">取消</el-button>\r\n      <el-button type=\"primary\" @click=\"isEditing ? handleUpdate() : handleAdd()\">确定</el-button>\r\n    </div>\r\n  </el-dialog>\r\n\r\n  <!-- 删除确认弹窗 -->\r\n  <el-dialog title=\"确认删除\" v-model=\"deleteDialogVisible\" width=\"30%\">\r\n    <span>确定要删除该用户吗？</span>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"deleteDialogVisible = false\">取消</el-button>\r\n      <el-button type=\"primary\" @click=\"confirmDelete\">确定</el-button>\r\n    </div>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue';\r\nimport request from \"@/utils/request\";\r\n\r\nconst name = ref('');\r\nconst currentPage = ref(1);\r\nconst pageSize = ref(10);\r\nconst total = ref(0);\r\nconst data = ref({ arr: [] });\r\nconst dialogFormVisible = ref(false);\r\nconst deleteDialogVisible = ref(false); // 控制删除确认弹窗\r\nconst isEditing = ref(false);\r\nconst deleteId = ref(null); // 保存待删除的用户ID\r\n\r\n// 表单数据\r\nconst formData = ref({\r\n  id: null,\r\n  username: '',\r\n  password: '',\r\n  name: '',\r\n  phone: '',\r\n  email: ''\r\n});\r\n\r\nconst handleSizeChange = (newSize) => {\r\n  pageSize.value = newSize;\r\n  fetchData(); // 更新数据\r\n};\r\n\r\nconst handleCurrentChange = (newPage) => {\r\n  currentPage.value = newPage;\r\n  fetchData(); // 更新数据\r\n};\r\n\r\n// 获取分页数据\r\nconst fetchData = async () => {\r\n  try {\r\n    const response = await request.get('/user/selectByPage', {\r\n      params: {\r\n        pageNum: currentPage.value,\r\n        pageSize: pageSize.value,\r\n        username: '',\r\n        name: name.value.trim()\r\n      }\r\n    });\r\n    data.value.arr = response.data.list;\r\n    total.value = response.data.total;\r\n  } catch (error) {\r\n    console.error('Error fetching data:', error);\r\n  }\r\n};\r\n\r\n// 搜索功能\r\nconst handleSearch = () => {\r\n  currentPage.value = 1;\r\n  fetchData();\r\n};\r\n\r\n// 显示新增弹窗\r\nconst showAddDialog = () => {\r\n  isEditing.value = false;\r\n  formData.value = { id: null, username: '', password: '', name: '', phone: '', email: '' };\r\n  dialogFormVisible.value = true;\r\n};\r\n\r\n// 显示编辑弹窗\r\nconst showEditDialog = (row) => {\r\n  isEditing.value = true;\r\n  formData.value = { ...row };\r\n  dialogFormVisible.value = true;\r\n};\r\n\r\n// 显示删除确认弹窗\r\nconst showDeleteDialog = (id) => {\r\n  deleteId.value = id;\r\n  deleteDialogVisible.value = true;\r\n};\r\n\r\n// 确认删除用户\r\nconst confirmDelete = async () => {\r\n  try {\r\n    await request.delete(`/user/delete/${deleteId.value}`);\r\n    deleteDialogVisible.value = false;\r\n    deleteId.value = null; // 清除删除ID\r\n    fetchData();\r\n  } catch (error) {\r\n    console.error('Error deleting user:', error);\r\n  }\r\n};\r\n\r\nonMounted(fetchData);\r\n</script>\r\n"],"mappings":"AAmEA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,OAAOC,OAAO,MAAM,iBAAiB;;;;;;;IAErC,MAAMC,IAAI,GAAGH,GAAG,CAAC,EAAE,CAAC;IACpB,MAAMI,WAAW,GAAGJ,GAAG,CAAC,CAAC,CAAC;IAC1B,MAAMK,QAAQ,GAAGL,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMM,KAAK,GAAGN,GAAG,CAAC,CAAC,CAAC;IACpB,MAAMO,IAAI,GAAGP,GAAG,CAAC;MAAEQ,GAAG,EAAE;IAAG,CAAC,CAAC;IAC7B,MAAMC,iBAAiB,GAAGT,GAAG,CAAC,KAAK,CAAC;IACpC,MAAMU,mBAAmB,GAAGV,GAAG,CAAC,KAAK,CAAC,CAAC,CAAC;IACxC,MAAMW,SAAS,GAAGX,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMY,QAAQ,GAAGZ,GAAG,CAAC,IAAI,CAAC,CAAC,CAAC;;IAE5B;IACA,MAAMa,QAAQ,GAAGb,GAAG,CAAC;MACnBc,EAAE,EAAE,IAAI;MACRC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZb,IAAI,EAAE,EAAE;MACRc,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE;IACT,CAAC,CAAC;IAEF,MAAMC,gBAAgB,GAAIC,OAAO,IAAK;MACpCf,QAAQ,CAACgB,KAAK,GAAGD,OAAO;MACxBE,SAAS,CAAC,CAAC,CAAC,CAAC;IACf,CAAC;IAED,MAAMC,mBAAmB,GAAIC,OAAO,IAAK;MACvCpB,WAAW,CAACiB,KAAK,GAAGG,OAAO;MAC3BF,SAAS,CAAC,CAAC,CAAC,CAAC;IACf,CAAC;;IAED;IACA,MAAMA,SAAS,GAAG,MAAAA,CAAA,KAAY;MAC5B,IAAI;QACF,MAAMG,QAAQ,GAAG,MAAMvB,OAAO,CAACwB,GAAG,CAAC,oBAAoB,EAAE;UACvDC,MAAM,EAAE;YACNC,OAAO,EAAExB,WAAW,CAACiB,KAAK;YAC1BhB,QAAQ,EAAEA,QAAQ,CAACgB,KAAK;YACxBN,QAAQ,EAAE,EAAE;YACZZ,IAAI,EAAEA,IAAI,CAACkB,KAAK,CAACQ,IAAI,CAAC;UACxB;QACF,CAAC,C