{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport request from \"@/utils/request\";\nexport default {\n  __name: 'HomeView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const name = ref('');\n    const currentPage = ref(1);\n    const pageSize = ref(10);\n    const total = ref(50);\n    const data = ref({\n      arr: []\n    });\n    const visible = ref(false);\n    const dialogFormVisible = ref(false);\n\n    // 新增用户的表单数据\n    const newUser = ref({\n      username: '',\n      password: '',\n      name: '',\n      phone: '',\n      email: '',\n      address: ''\n    });\n    const handleSizeChange = newSize => {\n      pageSize.value = newSize;\n    };\n    const handleCurrentChange = newPage => {\n      currentPage.value = newPage;\n    };\n    const fetchData = async () => {\n      try {\n        const response = await request.get('/user/selectAll');\n        data.value.arr = response.data;\n      } catch (error) {\n        console.error('Error fetching data:', error);\n      }\n    };\n\n    // 搜索功能\n    const handleSearch = async () => {\n      if (name.value.trim()) {\n        try {\n          const response = await request.get(`/user/selectByName/${name.value.trim()}`);\n          data.value.arr = response.data;\n        } catch (error) {\n          console.error('Error fetching data by name:', error);\n        }\n      } else {\n        fetchData();\n      }\n    };\n\n    // 提交新增用户的表单\n    const handleAdd = async () => {\n      try {\n        await request.post('/user/add', newUser.value);\n        dialogFormVisible.value = false; // 关闭弹窗\n        fetchData(); // 刷新列表\n      } catch (error) {\n        console.error('Error adding user:', error);\n      }\n    };\n\n    // 编辑功能\n    const handleEdit = row => {\n      console.log('Editing row:', row);\n    };\n\n    // 删除功能\n    const handleDelete = row => {\n      console.log('Deleting row:', row);\n      visible.value = true;\n    };\n    onMounted(fetchData);\n    